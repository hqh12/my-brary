<h2>Search Book</h2>
<form action="/books" method="GET">
    <div>
        <label>Tiitle</label>
        <input type="text" name="title" value="<%= searchOptions.title %>">
    </div>
    <div>
        <label>Publish After</label>
        <input type="date" name="publishAfter" value="<%= searchOptions.publishAfter %>">
    </div>
    <div>
        <label>Publish Before</label>
        <input type="date" name="publishBefore" value="<%= searchOptions.publishBefore %>">
    </div>
    <button type="submit">Search</button>
</form>
<div>
    <% books.forEach(book => {%>
        <img src="<%= book.coverImagePath %>" width="100" height="150">
    <%})%> 
</div>
